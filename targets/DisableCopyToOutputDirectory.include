<!--

    DisableCopyToOutputDirectory.targets
    
    Including this targets file disables the copy of items to the output directory.

-->

<Project>
    <PropertyGroup>
        <MSBuildAllProjects>$(MSBuildAllProjects);$(MSBuildThisFileFullPath)</MSBuildAllProjects>
    </PropertyGroup>

    <Target Name="_CopyFilesMarkedCopyLocal" />
    <Target Name="_CopyOutOfDateSourceItemsToOutputDirectory" />
    <Target Name="_CopyOutOfDateSourceItemsToOutputDirectoryAlways" />

</Project>
